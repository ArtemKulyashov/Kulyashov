public class University {
    public static void main(String[] args) {
        Student student1 = new Student("Artem", 22, 4.6);
        Student student2 = new Student("Sam", 23, 4.7);
        Student student3 = new Student("Rolan", 25, 3.2);
        Teacher teacher1 = new Teacher(student1, student2, student3);
        System.out.println("Information about students of teacher1:");
        teacher1.printInfoAboutStudents();
    }
}
